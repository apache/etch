<?xml version="1.0" encoding="utf-8"?>
<install xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://metreos.com/AppInstaller.xsd">
  <configuration>
    <configValue name="PhoneIP" displayName="Phone IP Address" format="String" description="The IP Address of a phone." defaultValue="" required="true" />
    <configValue name="PhoneUser" displayName="Phone Username" format="String" description="The name of a user associated with the phone." defaultValue="" required="true" />
    <configValue name="PhonePass" displayName="Phone Password" format="String" description="The password of a user associated with the phone." required="true" />
    <configValue name="AppServerIP" displayName="Application Server IP" format="String" description="The IP Address of the CUAE" required="true" />
